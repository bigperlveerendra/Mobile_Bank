        <activity
            android:name=".IndexActivity"
            android:label="@string/title_activity_index" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEmpActivity"
            android:label="@string/title_activity_add_emp" >
        </activity>
        <activity
            android:name=".EmpDelete"
            android:label="@string/title_activity_emp_delete" >
          
        </activity>
        <activity
            android:name=".GetEmp"
            android:label="@string/title_activity_get_emp" >
           
        </activity>
        <activity
            android:name=".UpdateEmp"
            android:label="@string/title_activity_update_emp" >
         
        </activity>
    </application>

</manifest>